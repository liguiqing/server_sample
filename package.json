{
  "name": "linggongpin",
  "version": "1.0.0",
  "description": "Description for Linggongpin",
  "private": true,
  "license": "UNLICENSED",
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "axios": "0.19.2",
    "date-fns": "2.14.0",
    "element-ui": "^2.13.2",
    "vue": "2.6.11",
    "vue-bus-ts": "^1.0.8",
    "vue-class-component": "7.2.3",
    "vue-cookie": "1.1.4",
    "vue-i18n": "8.18.2",
    "vue-infinite-loading": "2.4.5",
    "vue-property-decorator": "8.5.0",
    "vue-router": "3.3.4",
    "vue2-filters": "0.11.0",
    "vuelidate": "0.7.5",
    "vuex": "3.4.0",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/sinon": "9.0.4",
    "@types/vuelidate": "0.7.13",
    "@vue/cli-plugin-typescript": "4.4.4",
    "@vue/cli-service": "4.4.4",
    "@vue/test-utils": "1.0.3",
    "autoprefixer": "9.8.0",
    "babel-core": "7.0.0-bridge.0",
    "body-parser": "^1.19.0",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "copy-webpack-plugin": "6.0.2",
    "css-loader": "3.6.0",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "5.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "generator-jhipster-vuejs": "1.8.2",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-junit": "10.0.0",
    "jest-serializer-vue": "2.0.2",
    "jest-sonar-reporter": "2.0.0",
    "jest-vue-preprocessor": "1.7.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "10.2.10",
    "merge-jsons-webpack-plugin": "1.0.21",
    "mini-css-extract-plugin": "0.9.0",
    "node-notifier": "7.0.1",
    "numeral": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "portfinder": "1.0.26",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sass": "1.26.8",
    "sass-loader": "8.0.2",
    "sinon": "9.0.2",
    "terser-webpack-plugin": "3.0.5",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "tslib": "2.0.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.9.5",
    "url-loader": "4.1.0",
    "vue-jest": "3.0.5",
    "vue-loader": "15.9.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "engines": {
    "node": ">= 10.17.0",
    "npm": ">= 6.13.4"
  },
  "lint-staged": {
    "{,src/**/,webpack/}*.{md,json,js,ts,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,vue,json,js,ts,tsx,css,scss}\"",
    "lint": "tslint --project tsconfig.json -e 'node_modules/**'",
    "lint:fix": "npm run lint -- --fix",
    "cleanup": "rimraf build/resources/main/static/ build/resources/main/aot",
    "clean-www": "rimraf build/resources/main/static/app/{src,build/}",
    "start": "npm run webpack:dev",
    "start-tls": "npm run webpack:dev -- --env.tls",
    "serve": "npm run start",
    "build": "npm run webpack:prod",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --no-cache --config src/test/javascript/jest.conf.js",
    "jest:update": "npm run jest -- --updateSnapshot",
    "webpack:build": "npm run cleanup && npm run webpack:build:main",
    "webpack:build:main": "npm run webpack -- --config webpack/webpack.dev.js --progress --profile",
    "webpack:prod:main": "npm run webpack -- --config webpack/webpack.prod.js --profile",
    "webpack:prod": "npm run cleanup && npm run webpack:prod:main && npm run clean-www",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --progress --inline",
    "webpack:test": "npm run test",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "test": "npm run lint && npm run jest",
    "test-ci": "npm run lint && npm run jest:update",
    "test:watch": "npm run jest -- --watch",
    "mock": "npm run webpack-dev-server -- --config webpack/webpack.mock.js --progress --inline "
  },
  "jestSonar": {
    "reportPath": "build/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
